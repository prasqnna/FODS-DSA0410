import numpy as np
fuel_efficiency = np.array([25, 30, 28, 32, 27])
average_fuel_efficiency = np.mean(fuel_efficiency)
first_model_efficiency = fuel_efficiency[0]
second_model_efficiency = fuel_efficiency[1]
percentage_improvement = ((second_model_efficiency - first_model_efficiency) / first_model_efficiency) * 100
print("Average fuel efficiency across all car models:", average_fuel_efficiency, "miles per gallon")
print("Percentage improvement in fuel efficiency between the first and second car models:", percentage_improvement, "%")
 
